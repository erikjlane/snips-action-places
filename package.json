{
  "name": "snips-action-places",
  "version": "1.0.0",
  "description": "Snips action code for the Places app",
  "main": "dist/index.js",
  "scripts": {
    "start": "npm run lint && npm run build && npm run test",
    "build": "snips-toolkit build",
    "dev": "snips-toolkit dev",
    "test": "snips-toolkit test '^.*\\.spec\\.[jt]s$'",
    "test:sandbox": "snips-toolkit test -s '^.*\\.spec\\.[jt]s$'",
    "launch": "snips-toolkit run",
    "lint": "eslint \"**/*.[jt]s\""
  },
  "author": "Thomas Bouvier <contact@thomas-bouvier.io>",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^24.0.13",
    "@types/node": "^12.0.2",
    "@typescript-eslint/eslint-plugin": "^1.9.0",
    "@typescript-eslint/parser": "^1.9.0",
    "eslint": "^5.12.0",
    "eslint-plugin-jest": "^22.5.1",
    "typescript": "^3.3.3"
  },
  "dependencies": {
    "hermes-javascript": "^0.3.11",
    "snips-toolkit": "^0.1.4"
  },
  "sandbox": [
    "fs",
    "http",
    "https",
    "path",
    "stream",
    "tty",
    "url",
    "util",
    "zlib"
  ]
}
